{% extends "home.html" %}
{% block content-area %}
{% load getattribute %}
{% url 'dashapp:create_wfdoc'  doctype as create_wfdoc %}

    
{% include "wfdoc/filter_buttons_on_draft_list.html" with filter_buttons=filter_buttons doctype=doctype %}

              
        <table class="table table-striped table-hover">
            <thead >
                <tr style="background-color: #4CAF50;font-size:10px">
                    <th>invoice num</th>
                    <th>status</th>
                    {% for field_name in docdata_fields %}
	                    <th>{{ field_name }}</th>
	                {% endfor %}
	                <th>has draft</th>
	                <th>edit</th>
	                <th>delete</th>
                </tr>
            </thead>
            <tbody>
                {% for object in object_list reversed %}
                {% with object.doc_data as docdata %}
                    <tr style="font-size:10px">
                        <td>{{ object.name }}</td>
                        <td>{{ object.current_status }}</td>
                        {% for field_name in docdata_fields %}
                            <td>{{ docdata|getattribute:field_name }}</td>
                        {% endfor %}
                        <td>{{ object.has_draft_for_roles }}</td>
                        <td>
                            <a class="fa fa-edit" style="font-size:10px;color:red"  href="{% url 'dashapp:update_from_draft' object.name  %}" ></a>               
                        </td>
                        <td>
                            <a class="fa fa-remove" style="font-size:10px;color:red"  href="{% url 'dashapp:delete_wfmobj' objname object.name  %}" ></a>               
                        </td>
                    </tr>
                {% endwith %}
                {% endfor %}
                  
            </tbody>
        </table>
        <p>{{ result }}</p>
    </div>
    
{% endblock content-area %}


